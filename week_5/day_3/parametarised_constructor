//program using parametarised conctructors in oops


	#include<iostream>
	using namespace std;
	class constructor//creating class
	{
		string name;//declaring variable
		int age;//declaring variable
	public:
		constructor(string hh,int a)//constructor fucntion
		{
			name=hh;//initialising data
			age=a;
		}
		void display()
		{
			cout<<name<<endl;
			cout<<age<<endl;
		}

	};


	int main()
	{
		constructor obj("aswanth",55);//creating object
		obj.display();//calling function
	
	}
