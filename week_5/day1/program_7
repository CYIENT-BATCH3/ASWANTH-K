//WA C++ program to check the entered character is vowel or not


#include<iostream>//including header file
using namespace std;//including std library
int main()
{
	char ch;;//declaring variable

	cout<<"enter the character"<<endl;
    cin>>ch;//reading input
    if(ch =='A'|| ch=='E'||ch=='I'||ch=='O'||ch=='U'||ch =='A'+32|| ch=='E'+32||ch=='I'+32||ch=='O'+32||ch=='U'+32)//condition for checking vowel
    {
    	cout<<"it is vowel";
    }
    else
    	cout<<"it is not a vowel";


}
