//void pointer
1. start
2.decalre as void pointer
3.type cast to perticular datatype


#include <stdio.h>

int main() {
     int a=3;//declaring int variable
    float c=5.7;//decalring float variable
    void *p=&a;//declaring void pointer
    printf("value of a is %d",*(int *)p);//type casting to integer pointer
    p=&c;//storing float to void pointer
     printf("\nvalue of a is %f",*(float *)p);//type casting to float pointer
    return 0;
}
